<h1>Nossos Produtos</h1>
<div class="products">
    
    {{#each products}}
    <div class="product">
        
        
        <img src="{{this.image}}" alt="{{this.name}}">
        <h3>{{this.name}}</h3>
        <p>R$ {{this.price}}</p>
        <a href="/products/read/{{this._id}}">Detalhes</a>
        
        
    </div>
    {{/each}}
</div>